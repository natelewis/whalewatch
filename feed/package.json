{
  "name": "whalefeed",
  "version": "1.0.0",
  "description": "Trade data ingestion from Polygon.io to QuestDB",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/commands/ingest.ts",
    "ingest": "tsx src/commands/ingest.ts",
    "backfill": "tsx src/commands/backfill.ts",
    "reset": "tsx src/commands/reset.ts",
    "install-deps": "npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:catchup-demo": "tsx test-catchup-demo.ts",
    "test:catchup-unit": "tsx test-catchup-unit.ts",
    "test:catchup-behavior": "tsx test-catchup-behavior.ts",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "trading",
    "data",
    "polygon",
    "questdb",
    "websocket"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@whalewatch/shared": "file:../shared",
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.6",
    "node-cron": "^3.0.3",
    "p-limit": "^7.1.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "eslint": "^9.36.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.44.1"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/types/**/*.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(p-limit|yocto-queue)/)"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "maxWorkers": 1,
    "testTimeout": 30000
  }
}
